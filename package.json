{
  "name": "@gabrielandrade/smart-date-input",
  "version": "1.0.0",
  "description": "An intelligent date/time input component with natural language parsing, smart suggestions, and confidence scoring",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "react",
    "date",
    "input",
    "natural-language",
    "typescript",
    "component",
    "smart",
    "date-picker"
  ],
  "author": "Gabriel Andrade",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielgustavoandrade/smart-date-input.git"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "biome check src",
    "format": "biome format --write src",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache",
    "prepublishOnly": "bun run clean && bun run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "date-fns": "^3.0.0",
    "lucide-react": "^0.263.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  },
  "type": "module",
  "packageManager": "bun@1.2.10",
  "engines": {
    "node": ">=16.0.0"
  }
}